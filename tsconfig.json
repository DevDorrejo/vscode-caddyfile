{
    "compilerOptions": {
        "module": "CommonJS",
        "outDir": "out",
        "sourceMap": true,
        "target": "ES6",
        "lib": ["ES2015"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
    },
    "exclude": ["node_modules", "third_party"]
}
